    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "changeset": "changeset",
    "changeset:add": "changeset add",
    "changeset:status": "changeset status",
    "version": "changeset version && bun install",
    "release": "bun run check && bun run build && changeset publish",
